<!--
Provides a default 'form.additional.info' block on select pages when Mage_Captcha is unavailable.
-->
<config>
	<modules>
		<Clockworkgeek_Loginformfallback>
			<version>1.0.0</version>
		</Clockworkgeek_Loginformfallback>
	</modules>
	<frontend>
		<layout>
			<updates>
				<loginformfallback>
					<file>loginformfallback.xml</file>
				</loginformfallback>
			</updates>
		</layout>
	</frontend>
</config>
